{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start:dev": "NODE_ENV=development npm run debug -- npm run start",
    "prod": "NODE_ENV=production npm run start",
    "debug": "DEBUG=server:*",
    "dev": "npm run watch",
    "watch": "nodemon --config ./nodemon.json --exec npm run start:dev"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "mailgun-js": "^0.22.0",
    "morgan": "~1.9.0",
    "multer": "^1.4.1"
  },
  "devDependencies": {
    "dotenv": "^6.2.0",
    "eslint": "^5.12.0",
    "eslint-config-prettier": "^3.3.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3"
  }
}
